  frequency(4) = 2592000.,

  fields(1,5)  = 'THETA   ',
  fileName(5) = 'stateTHETA',
  frequency(5) = 2592000.,

  fields(1,6)  = 'SALT    ',
  fileName(6) = 'stateSALT',
  frequency(6) = 2592000.,

  fields(1,7)  = 'PsiVEL  ',
