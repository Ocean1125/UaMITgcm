  fields(1,8)  = 'EXFhs   ','EXFhl   ','EXFlwdn ','EXFswdn ','EXFtaux ','EXFtauy ','EXFuwind','EXFvwind','EXFatemp','EXFevap ','EXFpreci','EXFsnow ','EXFpress','EXFroff ',
  fileName(8) = 'stateEXF',
